{
  "name": "call-dir",
  "version": "1.2.1",
  "keywords": [
    "require",
    "directory",
    "files",
    "fs",
    "import",
    "autoloader"
  ],
  "description": "Loads files from a directory and executes a callback for each. All dot files and files without extension are ignored. Supports deep-loading.",
