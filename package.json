  "license": "MIT",
  "main": "dist/index.js",
  "repository": {
    "type": "git",
    "url": "https://github.com/Bartozzz/call-dir.git"
  },
  "bugs": {
    "url": "https://github.com/Bartozzz/call-dir/issues"
  },
  "devDependencies": {
    "babel-cli": "^6.26.0",
    "babel-core": "^6.26.3",
    "babel-eslint": "^8.2.3",
    "babel-plugin-add-module-exports": "^0.2.1",
    "babel-preset-env": "^1.7.0",
    "babel-preset-flow": "^6.23.0",
    "chai": "^4.1.2",
    "eslint": "^5.0.0",
    "eslint-config-prettier": "^2.9.0",
    "eslint-config-standard": "^11.0.0",
    "eslint-plugin-flowtype": "^2.49.3",
    "eslint-plugin-import": "^2.12.0",
    "eslint-plugin-node": "^6.0.1",
    "eslint-plugin-prettier": "^2.6.0",
    "eslint-plugin-promise": "^3.8.0",
    "eslint-plugin-standard": "^3.1.0",
    "flow-bin": "^0.74.0",
    "mocha": "^5.2.0",
    "prettier": "^1.13.5"
  },
  "scripts": {
    "test": "npm run test:eslint && npm run test:flow && npm run test:mocha",
    "test:flow": "flow",
    "test:mocha": "mocha --require babel-core/register",
    "test:eslint": "eslint --fix src",
    "clean": "rm -rf dist",
    "build": "babel src -d dist",
    "watch": "babel src -d dist -w",
    "prepare": "npm run clean && npm run test && npm run build"
  }
}
